# Auto detect text files and perform LF normalization
* text=auto
public static void quick_Sort(int []arr,int left,int right) {
        if(left>right) {
            return;
        }
        int i,j,temp,key;
        i=left;
        j=right;
        key=arr[left];
        while(i<j) {
            while(arr[j]>=key&&i<j) {
                j--;
            }
            while(arr[i]<=key&&i<j) {
                i++;
            }
            if(i<j) {
                temp=arr[j];
                arr[j]=arr[i];
                arr[i]=temp;
            }
        }
        arr[left]=arr[i];
        arr[i]=key;
        quick_Sort(arr, left, j-1);
        quick_Sort(arr, j+1, right);
    }
